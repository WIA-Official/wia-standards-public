{
  "name": "@wia/quantum",
  "version": "1.0.0",
  "description": "WIA-QUANTUM: Unified Quantum Computing Standard SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./backends": {
      "import": "./dist/backends/index.mjs",
      "require": "./dist/backends/index.js",
      "types": "./dist/backends/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "quantum",
    "quantum-computing",
    "qiskit",
    "cirq",
    "qsharp",
    "braket",
    "pyquil",
    "wia",
    "compiler",
    "transpiler"
  ],
  "author": "World Certification Industry Association (WIA) <contact@wia.family>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WIA-Official/wia-standards.git",
    "directory": "quantum/api/typescript"
  },
  "homepage": "https://wia.family",
  "bugs": {
    "url": "https://github.com/WIA-Official/wia-standards/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
