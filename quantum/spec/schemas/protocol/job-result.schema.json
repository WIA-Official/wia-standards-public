{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://wia.live/schemas/quantum/protocol/job-result.schema.json",
  "title": "WIA Quantum Job Result Payload",
  "description": "Payload schema for job_completed message",
  "type": "object",
  "required": ["jobId", "result"],
  "properties": {
    "jobId": {
      "type": "string",
      "description": "Job identifier"
    },
    "result": {
      "$ref": "../result.schema.json",
      "description": "Execution result (Phase 1 format)"
    },
    "timing": {
      "type": "object",
      "properties": {
        "queuedAt": {
          "type": "integer",
          "description": "Timestamp when job was queued"
        },
        "startedAt": {
          "type": "integer",
          "description": "Timestamp when execution started"
        },
        "completedAt": {
          "type": "integer",
          "description": "Timestamp when execution completed"
        },
        "executionTime": {
          "type": "number",
          "description": "Execution time in milliseconds"
        },
        "totalTime": {
          "type": "number",
          "description": "Total time including queue time"
        }
      }
    },
    "usage": {
      "type": "object",
      "properties": {
        "quantumSeconds": {
          "type": "number",
          "description": "Quantum compute time in seconds"
        },
        "creditsUsed": {
          "type": "number",
          "description": "Credits consumed"
        }
      }
    }
  }
}
